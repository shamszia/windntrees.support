@{
    ViewBag.Title = "Password Reset Failed";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Header
{
    <title>@LocaleResources.Views.Account.ResetFailed.Page</title>
}

<div id="webpage" class="row align-items-center login">
    <div class="col-4 m-auto align-content-center">
        <div class="card">
            <div class="card-header">
                <h3>
                    @LocaleResources.Views.Account.ResetFailed.Page
                </h3>
            </div>
            <div class="card-body">
                <p>
                    @LocaleResources.Views.Account.ResetFailed.Message
                </p>
            </div>
        </div>
    </div>
</div> 
